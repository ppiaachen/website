{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/assets/*": [
        "src/assets/*"
      ],
      "@/components/*": [
        "src/components/*"
      ],
      "@/data/*": [
        "src/data/*"
      ],
      "@/layouts/*": [
        "src/layouts/*"
      ],
      "@/lib/*": [
        "src/lib/*"
      ]
    }
  }
}