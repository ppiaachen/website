---
import { getEntry, render } from "astro:content";
import type { CollectionKey } from "astro:content";

interface Props {
  collectionKey: CollectionKey;
  slug: string;
}

const { collectionKey, slug } = Astro.props;
const entry =
  collectionKey && slug ? await getEntry(collectionKey, slug) : null;
const r = await render(entry);
---

{!!r && <r.Content />}
